/*EATE FUNCTION Main() RETURNS BOOLE*/
	BEGIN
		DECLARE rEnv          REFERENCE TO Environment.Variables."ComIbmDni"."DnqFinApplicationOutput";
		DECLARE chPayloadType CHAR rEnv."chPayloadType";
		DECLARE bMtfinExists  BOOLEAN rEnv."bMtfinExists";

		RETURN (chPayloadType = 'MTFIN') AND (NOT bMtfinExists);
	END;

