CREATE FUNCTION ComIbmDnqFinExtractFromMT021_IsFinAPDU(chFinMessage CHAR) RETURNS BOOLEAN
BEGIN

	DECLARE bIsFinAPDU BOOLEAN FALSE;

	IF (LENGTH(chFinMessage) > 4) AND ((SUBSTRING(chFinMessage FROM 1 FOR 4) = '{1:F') OR (SUBSTRING(chFinMessage FROM 1 FOR 4) = '{1:A')) THEN
		SET bIsFinAPDU = TRUE;
	END IF;

RETURN bIsFinAPDU;
END;