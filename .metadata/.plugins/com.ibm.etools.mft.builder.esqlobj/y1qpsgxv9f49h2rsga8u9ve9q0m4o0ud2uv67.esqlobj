CREATE FILTER MODULE DnqErCommon_EmptyPayload
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		DECLARE bEmptyPayload BOOLEAN FALSE;
		DECLARE bodyDomain    CHAR    FIELDNAME(Body);

		IF ((bodyDomain <> 'XMLNSC') AND (bodyDomain <> 'BLOB')) THEN
			SET bEmptyPayload = TRUE;
		END IF;

		RETURN bEmptyPayload;
	END;
END MODULE;