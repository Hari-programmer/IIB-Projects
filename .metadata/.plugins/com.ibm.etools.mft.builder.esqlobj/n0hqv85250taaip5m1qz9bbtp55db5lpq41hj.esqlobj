/*EATE FUNCTION Main() RETURNS BOOLE*/
	BEGIN
		
		
		DECLARE i INTEGER 1;
	    FOR InRef AS InputRoot.MRM[]  DO
	    	DECLARE List INTEGER CARDINALITY(InRef[]);
	    		WHILE i<=List DO
	    			SET OutputLocalEnvironment.Destination.HTTP.RequestURL = 'https://api.dictionaryapi.dev/api/v2/entries/en/' 
                	                                              || InRef[i];
	   				SET OutputLocalEnvironment.Destination.HTTP.RequestLine.Method = 'GET';
	    		PROPAGATE TO TERMINAL 'out';
	    
	    		SET i = i+1;
	    	END WHILE;
	    END FOR;
		
		
