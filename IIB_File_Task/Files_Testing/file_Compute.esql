BROKER SCHEMA Files_Testing


CREATE COMPUTE MODULE file_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		-- CALL CopyMessageHeaders();
		-- CALL CopyEntireMessage();
		
		DECLARE EV REFERENCE TO Environment.Variables;
		CREATE LASTCHILD OF EV.req DOMAIN 'XMLNSC' PARSE (InputRoot.BLOB.BLOB);
		
		SET OutputRoot.XMLNSC.ROOT.NAME = EV.req.XMLNSC.details.name;
		SET OutputRoot.XMLNSC.ROOT.PHN = EV.req.XMLNSC.details.phn;
		SET OutputRoot.XMLNSC.ROOT.FROM = EV.req.XMLNSC.details.from;
		
		
		
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyMessageHeaders() BEGIN
		DECLARE I INTEGER 1;
		DECLARE J INTEGER;
		SET J = CARDINALITY(InputRoot.*[]);
		WHILE I < J DO
			SET OutputRoot.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot = InputRoot;
	END;
END MODULE;
